<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.alfonzm.logoquiz"
    android:versionCode="1"
    android:versionName="0.2" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="16" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="Pinoy Logo Quiz"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.alfonzm.logoquiz.MainMenu"
            android:label="Pinoy Logo Quiz" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.alfonzm.logoquiz.LogoQuiz"
            android:label="@string/title_activity_logo_quiz" >
        </activity>
        <activity
            android:name="com.alfonzm.logoquiz.LogoAnswer"
            android:label="@string/title_activity_logo_answer" >
        </activity>
    </application>

</manifest>